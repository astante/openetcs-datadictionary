            description="DataDictionary based on SRS Subset-026-7 and Subset-026-8"
            name="openETCSDataDictionary"
            path="pathmap://DATADICTIONARY_LIBRARIES/DataDictionary.library.uml"
            provider="Fraunhofer ESK">
      </library>
   </extension>

</plugin>
